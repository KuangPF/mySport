<script type="text/ng-template" id="baseInfo.html">
    <div class="content  col-md-12">
        <div class="page-header">
            <h1>基本信息</h1>
        </div>
        <div class="">
            <form role="form" class="form-horizontal" method="post" action="/information/save">
                <div class="form-group">
                    <label for="username" class="control-label col-md-1" >用户名:&nbsp;</label>
                    <div class="col-md-5 input-group">
                        <input id="username" type="text" value="<%=userInfo.username%>" class="form-control" disabled name="username">
                        <span class="input-group-addon">该用户名为用户登录，不可修改</span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="nickname" class="control-label col-md-1">昵称:&nbsp;</label>
                    <div class="col-md-5 input-group">
                        <input id="nickname" type="text" class="form-control" placeholder="请输入昵称" value="<%=userInfo.userNickName%>" name="userNickName">
                        <span class="input-group-addon">
                            <span class="glyphicon glyphicon-heart"></span>
                        </span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="motto" class="control-label col-md-1" >个性签名:&nbsp;</label>
                    <div class="col-md-5 input-group">
                        <input id="motto" type="text" class="form-control" placeholder="请输入个性签名" value="<%=userInfo.userSignature%>" name="userSignature">
                        <span class="input-group-addon">
                            <span class="glyphicon glyphicon-edit"></span>
                        </span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="email" class="control-label col-md-1" >邮箱:&nbsp;</label>
                    <div class="col-md-5 input-group">
                        <input id="email" type="email" class="form-control" placeholder="请输入邮箱" value="<%=userInfo.usernameEmail%>" name="usernameEmail">
                        <span class="input-group-addon">
                            <span class="glyphicon glyphicon-envelope"></span>
                        </span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="name" class="control-label col-md-1">真实姓名:&nbsp;</label>
                    <div class="col-md-5 input-group">
                        <input id="name" type="text" class="form-control" placeholder="请输入姓名" value="<%=userInfo.userActualName%>" name="userActualName">
                        <span class="input-group-addon">
                            <span class="glyphicon glyphicon-user"></span>
                        </span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="ID" class="control-label col-md-1" >身份证号码:&nbsp;</label>
                    <div class="col-md-5 input-group">
                        <input id="ID" type="text" class="form-control" placeholder="请输入身份证号码" value="<%=userInfo.usernameIDNum%>" name="usernameIDNum">
                        <span class="input-group-addon">
                            <span class="glyphicon glyphicon-credit-card"></span>
                        </span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="dtp_input2" class="control-label col-md-1 ">生日：&nbsp;</label>
                    <div class="input-group date form_date col-md-5" data-date="" data-date-format="dd MM yyyy" data-link-field="dtp_input2" data-link-format="yyyy-mm-dd">
                        <input class="form-control"  type="text" value="<%=userInfo.userBirthDay%>" name="userBirthDay">
                        <span class="input-group-addon"><span class="glyphicon glyphicon-remove"></span></span>
                        <span class="input-group-addon"><span class="glyphicon glyphicon-calendar" onclick="getDate()"></span></span>
                    </div>
                    <input type="hidden" id="dtp_input2" value="" /><br/>
                </div>
                <div class="form-group">
                    <div class="col-md-1"></div>
                    <input type="reset" class="btn btn-primary ml10">
                    <input type="submit" class="btn btn-danger" value="保存">
                </div>
            </form>
        </div>
    </div>
</script>
<script type="text/ng-template" id="deliver_address.html">
    <div class="content  col-md-12">
        <div class="page-header">
            <h1>收货地址</h1>
        </div>
        <div class="address_info">
                <form class="form-horizontal">
                        <div class="form-group">
                            <label for="city-picker3" class="control-label col-md-1">所在地区：&nbsp;</label>
                            <div>
                                <input id="city-picker3" class="form-control" readonly type="text"data-toggle="city-picker" onclick="getCity()" style="width: 20%">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-1"></div>
                            <button class="btn btn-primary btn-sm" id="reset" type="button">重置</button>
                            <button class="btn btn-danger btn-sm ml10" id="destroy" type="button">确定</button>
                        </div>
                        <div class="form-group">
                            <label for="detail-address" class="control-label col-md-1">详细地址：&nbsp;</label>
                            <div class="col-md-5 input-group">
                                <textarea id="detail-address" class="form-control"></textarea>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="postalNum" class="control-label col-md-1">邮政编码：&nbsp;</label>
                            <div class="col-md-5 input-group">
                                <input type="text" id="postalNum" class="form-control" placeholder="如您不清楚邮递区号，请填写000000">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="recipientName" class="control-label col-md-1">收件人姓名：&nbsp;</label>
                            <div class="col-md-5 input-group">
                                <input type="text" id="recipientName" class="form-control" placeholder="长度不能超过10个字符">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="phoneNum" class="control-label col-md-1">手机号码：&nbsp;</label>
                            <div class="col-md-5 input-group">
                                <input type="text" id="phoneNum" class="form-control" placeholder="请输入手机号码">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="defaultAddress" class="control-label col-md-1"></label>
                            <div class="col-md-5 input-group">
                                <input type="checkbox" id="defaultAddress">&nbsp;&nbsp;设置为默认收货地址
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-1"></div>
                            <input type="reset" class="btn btn-primary">
                            <input type="submit" class="btn btn-danger ml10" value="保存">
                        </div>
                </form>
        </div>
        <div>
            <div class="page-header">
                <h3>已保存收货地址</h3>
            </div>
            <div class="col-md-10">
                <table class="table table-striped table-bordered table-hover">
                    <thead>
                    <tr>
                        <th>收货人</th>
                        <th>所在地区</th>
                        <th>详细地址</th>
                        <th>邮编</th>
                        <th>电话/手机</th>
                        <th>操作</th>
                        <th>默认地址</th>
                    </tr>
                    </thead>
                    <tbody>
                        <tr class="active">
                            <td>匡鹏飞</td>
                            <td>重庆 重庆市 南岸区</td>
                            <td>重庆市南岸区重庆邮电大学崇文路2号</td>
                            <td>400065</td>
                            <td>18883846762 </td>
                            <td><a href="#">修改</a>&nbsp;||&nbsp;<a href="#">删除</a></td>
                            <td><button class="btn btn-danger btn-sm">设为默认地址</button></td>
                        </tr>
                        <tr class="active">
                            <td>余薇</td>
                            <td>重庆 重庆市 南岸区</td>
                            <td>重庆市南岸区重庆邮电大学崇文路2号</td>
                            <td>400065</td>
                            <td>18883866970</td>
                            <td><a href="#">修改</a>&nbsp;||&nbsp;<a href="#">删除</a></td>
                            <td><span class="glyphicon glyphicon-map-marker"></span> <span>默认地址</span></td>
                        </tr>
                    </table>
            </div>
        </div>
    </div>
</script>
<script type="text/ng-template" id="changePassword.html">
    <div class="content  col-md-12">
        <div class="page-header">
            <h1>修改登录密码</h1>
        </div>
        <div class="">
            <form role="form" class="form-horizontal ">
                <div class="form-group">
                    <label for="oldPassword" class="control-label col-md-1">现在的密码：&nbsp;</label>
                    <div class="input-group col-md-5">
                        <input id="oldPassword" type="password" class="form-control" placeholder="请输入原密码">
                    </div>
                </div>
                <div class="form-group">
                    <label for="newPassword" class="control-label col-md-1">设置新密码：&nbsp;</label>
                    <div class="input-group col-md-5">
                        <input id="newPassword" type="password" class="form-control" placeholder="请输入新密码">
                    </div>
                </div>
                <div class="form-group">
                    <label for="repeatNewPassword" class="control-label col-md-1">重复新密码：&nbsp;</label>
                    <div class="input-group col-md-5">
                        <input id="repeatNewPassword" type="password" class="form-control" placeholder="请重复新密码">
                    </div>
                </div>
                <div class="form-group">
                    <label for="verificationCode" class="control-label col-md-1">验证码：&nbsp;</label>
                    <div class="input-group col-md-5">
                        <input id="verificationCode" type="password" class="form-control" placeholder="请输入验证码">
                        <span class="input-group-btn">
                            <button class="btn btn-primary msgs msgsOne" type="button" onclick="getVerificationCodeOne()">获取验证码</button>
                        </span>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-1"></div>
                    <input type="reset" class="btn btn-primary" value="取消">
                    <input type="submit" class="btn btn-danger ml10" value="保存">
                </div>
            </form>
        </div>
    </div>
</script>
<script type="text/ng-template" id="bindPhone.html">
    <div class="content  col-md-12">
        <div class="page-header">
            <h1>手机绑定</h1>
        </div>
        <div class="panel-group">
            <div class="phoneBind">
                <div class="">
                    <div class="col-md-1" style="width: 10%">
                        <h5>手机没绑定？</h5>
                    </div>
                    <button id="phoneBind_btn" class="btn btn-primary" data-toggle="collapse" href="#collapseOne">
                        点击绑定
                    </button>
                </div>
                <div class="panel panel-default col-md-10" id="phoneBind">

                    <div id="collapseOne" class="panel-collapse collapse">
                        <div class="panel-body">
                            <form role="form" class="form-horizontal">
                                <div class="form-group">
                                    <label for="bindPhoneNum" class="control-label col-md-1" >手机号码:&nbsp;</label>
                                    <div class="col-md-5 input-group">
                                        <input id="bindPhoneNum" type="text" class="form-control" placeholder="请输入要绑定的手机号码">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="bindPassword" class="control-label col-md-1" >登录密码:&nbsp;</label>
                                    <div class="col-md-5 input-group">
                                        <input id="bindPassword" type="text" class="form-control" placeholder="请输入登录密码">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="bindVerificationCode" class="control-label col-md-1" >验证码:&nbsp;</label>
                                    <div class="col-md-5 input-group">
                                        <input id="bindVerificationCode" type="text" class="form-control" placeholder="请输入验证码">
                                        <span class="input-group-btn">
                                        <button class="btn btn-primary msgs msgs1 msgsTwo" type="button" onclick="getVerificationCodeTwo()">获取验证码</button>
                                    </span>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-1"></div>
                                    <input id="phoneBind_cancel" type="reset" class="btn btn-primary" value="重置">
                                    <input type="submit" class="btn btn-danger ml10" value="绑定手机">
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel-group">
            <div class="ModifyPhoneBinding">
                <div class="">
                    <div class="col-md-1" style="width:10%">
                        <h5>手机已绑定？</h5>
                    </div>
                    <button id="ModifyPhoneBinding_btn" class="btn btn-danger" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
                        点击修改
                    </button>
                </div>
                <div class="panel panel-default col-md-10" id="ModifyPhoneBinding">

                    <div id="collapseTwo" class="panel-collapse collapse ">
                        <div class="panel-body">
                            <form role="form" class="form-horizontal" action="">
                                <div class="form-group">
                                    <label for="ModifyPhoneBindPhoneNum" class="control-label col-md-1" >已绑手机:&nbsp;</label>
                                    <div class="col-md-5 input-group">
                                        <input id="ModifyPhoneBindPhoneNum" type="text" class="form-control" placeholder="请输入已绑定的手机号码">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ModifyPhoneBindVC" class="control-label col-md-1" >验证码:&nbsp;</label>
                                    <div class="col-md-5 input-group">
                                        <input id="ModifyPhoneBindVC" type="text" class="form-control" placeholder="请输入已绑定手机验证码">
                                        <span class="input-group-btn">
                                        <button class="btn btn-primary msgs msgsThree" type="button" onclick="getVerificationCodeThree()">获取验证码</button>
                                    </span>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ModifyPhoneBindingPhoneNum" class="control-label col-md-1" >绑定手机:&nbsp;</label>
                                    <div class="col-md-5 input-group">
                                        <input id="ModifyPhoneBindingPhoneNum" type="text" class="form-control" placeholder="请输入要绑定的手机号码">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ModifyPhoneBindingVC" class="control-label col-md-1" >验证码:&nbsp;</label>
                                    <div class="col-md-5 input-group">
                                        <input id="ModifyPhoneBindingVC" type="text" class="form-control" placeholder="请输入要绑定手机的验证码">
                                        <span class="input-group-btn">
                                        <button class="btn btn-primary msgs msgsFour" type="button" onclick="getVerificationCodeFour()">获取验证码</button>
                                    </span>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ModifyPhoneBindingPassword" class="control-label col-md-1" >登录密码:&nbsp;</label>
                                    <div class="col-md-5 input-group">
                                        <input id="ModifyPhoneBindingPassword" type="text" class="form-control" placeholder="请输入登录密码">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-1"></div>
                                    <input id="ModifyPhoneBinding_cancel" type="reset" class="btn btn-primary" value="重置">
                                    <input type="submit" class="btn btn-danger ml10" value="修改绑定">
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
